UART.init 16 17 115200
rul = 0
mot = 0
add = 20

loop 11 1 {

repeat 10 {
  #pinmode 23 OUTPUT
  #blink 23

  ton = {mot}
  if {ton} < 0 ton = -1 * {mot}
  tone 23 {ton}

  mot += {add}
  if {mot} > 300 add = -1 * {add}
  if {mot} < -300 add = -1 * {add}

  echo add={add} mot={mot} ton={ton}

  IBAN.send 0 {mot}
  usleep 99
}